<%= simple_form_for([@flat, @booking]) do |f| %>
    <div>
      <div class="form-inline input-form">
        <%= f.input :move_in_date, label: 'Start', as: :string,   input_html: { id:"start_date", class: "input-fields"} %>
        <%= f.input :move_out_date, label: 'End', as: :string,  input_html: { id:"end_date", class: "input-fields"}%>
      </div>
      <br>
      <%= f.submit class: "btn-book text-alignment", id: "  book" %>
      <% end %>
</div>
